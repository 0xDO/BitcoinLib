<?xml version="1.0" encoding="utf-8"?>

<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <appSettings>
    <add key="DaemonUrl" value="http://localhost.:8332" />
    <add key="TestNetDaemonUrl" value="http://localhost.:18332" />
    <add key="RpcUsername" value="myRpcUsername" />
    <add key="RpcPassword" value="myRpcPassword" />
    <add key="RpcRequestTimeoutInSeconds" value="5" />
    <add key="WalletPassword" value="MyWalletPassword_PleaseDoNotStoreMeHere" />
    <add key="UseTestNet" value="false" />
    <add key="ExtractMyPrivateKeys" value="false" />
  </appSettings>
</configuration>